@startuml

package threeLayer {
    class PaperController

    class PaperService {
        assemblePaper()
        reassemblePaper()
    }


    class Paper {
        quizId: PaperId
        quizzes: List<Quiz>
    }

    class PaperDAO {
        save()
    }


    PaperController ..> PaperService
    PaperService ..> Paper
    PaperService ..> PaperDAO
    PaperDAO .left.> Paper
}
@enduml